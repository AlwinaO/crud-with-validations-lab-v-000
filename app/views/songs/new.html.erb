<h2>Song Form</h2>

<%= form_for(@song) do |f| %>
  <label>Song title:</label>
  <%= f.text_field :title %><br>

  <label>Release Year:</label>
  <%= f.text_field :release_year %><br>

  <label>Released:</label>
  <%= f.text_field :title %><br>

  <label>Artist Name:</label>
  <%= f.text_field :artist_name %><br>

  <label>Genre:</label>
  <%= f.text_field :genre %><br>

  <%= f.submit %>
<% end %>
